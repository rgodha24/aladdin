{
	"name": "aladdin",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"postinstall": "svelte-kit sync && prisma generate",
		"generate": "kysely-codegen && prisma generate",
		"dev": "sst bind vite dev",
		"build": "vite build && cp prisma/schema.prisma .svelte-kit/svelte-kit-sst/server/lambda-handler/schema.prisma",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"sst:deploy": "sst deploy",
		"sst:dev": "sst dev"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^1.2.0",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.15.5",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"autoprefixer": "^10.4.7",
		"aws-cdk-lib": "2.72.1",
		"constructs": "10.1.156",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"kysely-codegen": "^0.10.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "4.9.0",
		"sst": "^2.8.15",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-kit-sst": "^2.8.15",
		"svelte-preprocess": "^5.0.3",
		"sveltekit-superforms": "^0.7.1",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^2.0.0",
		"@planetscale/database": "^1.7.0",
		"@prisma/client": "4.9.0",
		"better-sqlite3": "^8.3.0",
		"kysely": "^0.24.2",
		"kysely-planetscale": "^1.3.0",
		"lucia-auth": "^1.4.0"
	}
}
